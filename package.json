{
    "name": "full-stack",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "dev": "webpack --config ./webpack/dev.config.js --watch",
        "prod": "webpack -p --config ./webpack/prod.config.js --watch",
        "nodemon": "lsof -t -i tcp:3001 | xargs kill ; lsof -t -i tcp:8081 | xargs kill ; nodemon --delay 1500ms",
        "start": "DEBUG=moon:* node ./bin/www & node ./api/src/apiServer.js",
        "deploy": "webpack --config ./webpack/prod.config.js && eb deploy",
        "pretest": "mongod --fork --logpath /dev/null",
        "test": "NODE_ENV=test && cd api && DEBUG=none mocha --timeout 120000 \"test/**/*.js\" --exit || true",
        "coverage": "NODE_ENV=test && cd api && nyc mocha --timeout 120000 --exit || true",
        "posttest": "mongo admin --eval 'db.dropDatabase(); db.shutdownServer()' > /dev/null"
    },
    "events": {
        "restart": "lsof -t -i tcp:3001 | xargs kill ; lsof -t -i tcp:8081 | xargs kill"
    },
    "directories": {
        "test": "./api/test"
    },
